{
  "name": "nikthequik",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "4.4.4"
  },
  "repository": {},
  "private": true,
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "build": "npm run clean && mkdir build && npm run build:html && npm run build:css",
    "clean": "rm -rf build",
    "clean:html": "rm -f build/index.html",
    "clean:css": "rm -f build/index.css build/index.css.map",
    "build:html": "npm run clean:html && cp index.html build/",
    "build:css": "npm run clean:css && lessc --source-map css/index.less build/index.css"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "globals": {
      "require": false,
      "beforeEach": false,
      "it": false,
      "expect": false,
      "xdescribe": false,
      "describe": false
    },
    "rules": {
      "no-undef": 1,
      "quotes": [
        2,
        "single"
      ]
    },
    "parsers": "babel-eslint"
  },
  "author": "nik",
  "dependencies": {
    "bootstrap": "3.3.6"
  },
  "devDependencies": {
    "less": "^2.7.1"
  }
}
